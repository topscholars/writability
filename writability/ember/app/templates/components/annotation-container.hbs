{{#if isStudent}}
    <p id="anno-column-info">Your essay will auto-save!</p>
{{/if}}

{{#each annotationGroup in existingAnnotationGroups}}
	{{annotation-groupcontainer group=annotationGroup isStudent=isStudent}}
{{/each}}

{{#if newAnnotation}}
	{{annotation-createbox annotation=newAnnotation tags=tags hasSavedAnnotation="hasSavedAnnotation"}}
{{/if}}
