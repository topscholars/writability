<nav class="details-nav">
    {{#each tab in view.tabs}}
        <div id="tab-{{unbound tab.key}}" {{action "selectTab" tab.key
        target="view"}} class="tab-header">
            {{tab.title}}
        </div>
    {{/each}}
</nav>

<div class="tab-content">
    {{view view.tabsViewClass viewName="tabsView"}}
</div>
